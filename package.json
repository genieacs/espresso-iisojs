{
  "name": "yebool",
  "version": "1.0.1",
  "description": "Implementation of Espresso-II method for heuristic minimization of single output boolean functions",
  "type": "module",
  "main": "dist/yebool.cjs",
  "exports": {
    ".": {
      "import": "./dist/yebool.mjs",
      "require": "./dist/yebool.cjs"
    },
    "./jsbi": {
      "import": "./dist/yebool-jsbi.mjs",
      "require": "./dist/yebool-jsbi.cjs"
    },
    "./index.d.ts": "./index.d.ts"
  },
  "files": [
    "dist/yebool.mjs",
    "dist/yebool.cjs",
    "dist/yebool-jsbi.mjs",
    "dist/yebool-jsbi.cjs",
    "index.d.ts"
  ],
  "scripts": {
    "test": "ts-node test/test.ts",
    "lint-eslint": "eslint --fix \"src/*.ts\"",
    "lint-prettier": "prettier --parser typescript --write \"src/*.ts\"",
    "lint-tsc": "tsc --noEmit",
    "lint": "npm run lint-eslint && npm run lint-prettier && npm run lint-tsc",
    "build": "rollup -c"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/genieacs/yebool.git"
  },
  "keywords": [
    "logic",
    "sat"
  ],
  "author": "Zaid Abdulla <zaid@genieacs.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/genieacs/yebool/issues"
  },
  "homepage": "https://github.com/genieacs/yebool#readme",
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^8.1.0",
    "@rollup/plugin-typescript": "^5.0.0",
    "@types/node": "^14.0.13",
    "@typescript-eslint/eslint-plugin": "^3.4.0",
    "@typescript-eslint/parser": "^3.4.0",
    "eslint": "^7.3.1",
    "eslint-config-prettier": "^6.11.0",
    "prettier": "^2.0.5",
    "rollup": "^2.18.0",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.5"
  },
  "optionalDependencies": {
    "jsbi": "^3.1.3"
  }
}
